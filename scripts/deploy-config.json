{
  "deployment": {
    "bucketName": "kareem-portfolio-site",
    "region": "us-east-1",
    "profile": "default",
    "cacheSettings": {
      "html": "max-age=300",
      "css": "max-age=86400",
      "js": "max-age=86400",
      "assets": "max-age=2592000"
    },
    "cloudfront": {
      "priceClass": "PriceClass_100",
      "compress": true,
      "redirectToHttps": true
    }
  },
  "verification": {
    "checkFiles": [
      "index.html",
      "styles.css", 
      "script.js",
      "config.js",
      "assets/kareem-photo.jpg"
    ],
    "testFunctions": [
      "Theme toggle",
      "Progress bars animation",
      "Smooth scrolling",
      "Project filtering",
      "Mobile responsiveness"
    ]
  }
}
